      ]
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "echo \"(optional) skipped in web deploy\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/pg": "^8.15.5",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "pg": "^8.16.3"
  }
}
