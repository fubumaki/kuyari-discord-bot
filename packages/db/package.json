{
  "name": "@kuyari/db",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*",
      "require": "./src/*"
    }
